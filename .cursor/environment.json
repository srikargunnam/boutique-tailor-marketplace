{
    "install": "npm install",
    "start": "expo start --clear",
    "terminals": [
        {
            "name": "Supabase",
            "command": "supabase start"
        },
        {
            "name": "Build Android",
            "command": "expo build:android -t apk"
        },
        {
            "name": "Build iOS",
            "command": "expo build:ios -t simulator"
        }
    ]
}
